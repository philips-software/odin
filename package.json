{
  "name": "@philips-software/odin",
  "version": "2.0.0",
  "description": "On-demand dependency injection",
  "author": "Philips EMR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:philips-software/odin"
  },
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rimraf --glob .coverage dist **/.eslintcache **/*.log **/*.tsbuildinfo",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --cache --max-warnings 0",
    "release": "semantic-release",
    "test": "vitest run --coverage",
    "test:dev": "vitest dev --coverage",
    "test:ui": "vitest dev --coverage --ui",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/debug": "^4.1.9",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.6",
    "eslint": "8.50.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jest-formatting": "3.1.0",
    "jsdom": "22.1.0",
    "rimraf": "5.0.5",
    "semantic-release": "22.0.5",
    "semantic-release-license": "1.0.3",
    "semantic-release-plugin-update-version-in-files": "1.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-dts": "3.6.0",
    "vite-plugin-typescript-transform": "1.2.2",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": ">= 18.17",
    "npm": ">= 9"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
