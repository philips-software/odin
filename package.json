{
  "name": "@odinjs/odin",
  "version": "2.0.0",
  "description": "On-demand dependency injection",
  "author": "Philips EMR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:philips-software/odin"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf --glob lib **/.eslintcache **/*.log **/*.tsbuildinfo **/coverage",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --cache --max-warnings 0",
    "release": "semantic-release",
    "test": "jest --config jest.config.js",
    "test:clear-cache": "npm run test -- --clearCache",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "28.1.3",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.43.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-jest-formatting": "3.1.0",
    "jest": "28.1.1",
    "rimraf": "5.0.1",
    "semantic-release": "21.0.5",
    "semantic-release-license": "1.0.3",
    "ts-jest": "28.0.5",
    "typescript": "5.1.3"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 9"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
