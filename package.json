{
  "name": "@odinjs/odin",
  "version": "2.0.0",
  "description": "On-demand dependency injection",
  "author": "Philips EMR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:philips-software/odin"
  },
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc --build tsconfig-refs.json",
    "clean": "rimraf --glob lib **/.eslintcache **/*.log **/*.tsbuildinfo **/coverage",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --cache --max-warnings 0",
    "release": "semantic-release",
    "test": "vitest run --coverage",
    "test:dev": "vitest dev --coverage",
    "test:ui": "vitest dev --coverage --ui"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/debug": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-v8": "0.33.0",
    "@vitest/ui": "0.33.0",
    "eslint": "8.45.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "rimraf": "5.0.1",
    "semantic-release": "21.0.7",
    "semantic-release-license": "1.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "vite-plugin-typescript-transform": "1.2.2",
    "vitest": "0.33.0"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 9"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
